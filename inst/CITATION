citHeader("To cite betaBayes in publications use:")

citEntry(entry = "Article",
  title        = "Bayesian beta regression for bounded responses with unknown supports",
  author       = personList(as.person("Haiming Zhou"),
                   as.person("Xianzheng Huang")),
  year         = "2022",
  journal      = "Computational Statistics & Data Analysis",
  volume       = "167",
  pages        = "107345",
  textVersion  =
  paste("Zhou, H. and Huang, X. (2022).",
        "Bayesian beta regression for bounded responses with unknown supports.", 
        "Computational Statistics & Data Analysis, 167, 107345.")
)
